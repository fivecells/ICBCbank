	function unParamSafe(str,name){
		var tmp = {};
		var arr = str.split("&");
		for(var i=0;i<arr.length;i++){
			var idx = arr[i].indexOf("=");
			tmp[arr[i].substring(0,idx)] = arr[i].substring(idx+1);
		}
		if(!tmp[name])
			tmp[name] =  tmp["Edit"]; 
		return tmp;
	}
	
